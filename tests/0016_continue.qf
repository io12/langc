bool passed_test(void)
{
	var I32 i;
	var I32 j;

	j = 0;
	for (i = 0; i < 16; i++) {
		j--;
		if (j < -5 || j < -2) {
			break;
		} else {
			continue;
			j += 11111;
			i += 22222;
		}
	}
	if (i != 2 || j != -3) {
		return false;
	}
	do {
		i++;
		j += 2;
		if (i == 10 && j == 13) {
			break;
		} else {
			continue;
			j += 11111;
			i += 22222;
		}
	} while (true);
	if (i != 10 || j != 13) {
		return false;
	}
	while (true) {
		i *= 10;
		j /= 10;
		if (i == 100000 && j == 0) {
			break;
		} else {
			continue;
			j += 11111;
			i += 22222;
		}
	}
	if (i != 100000 || j != 0) {
		return false;
	}
	return true;
}
